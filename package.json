  "scripts": {
    "postinstall": "node ./build/postinstall.mjs",
    "start": "cross-env DEBUG=1 electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint '**/*.{ts,vue}' && echo '0 linting error' && vue-tsc --noEmit --project tsconfig.lint.json && echo '0 TypeScript error' && stylelint '**/*.{css,vue}' && echo '0 CSS error'",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "lint:watch": "node build/lint-watch.mjs",
    "credits": "npx @opengovsg/credits-generator",
    "cli": "tsx src/cli/main.ts",
    "build:cli": "node build/build-cli.mjs",
    "test": "vitest --run",
    "test:ai": "vitest --run --silent --reporter=./tests/reporter/silent.ts",
    "test:notify": "vitest --watch --reporter=default --reporter=./tests/reporter/notify.ts",
    "test:watch": "vitest --watch --silent",
    "test:ci": "vitest --coverage.enabled=true --coverage.reporter='text-summary' --silent",
    "test:ui": "vitest --ui --coverage.enabled=true --silent",
    "test:e2e": "WITSY_HOME=/tmp vitest --config ./vitest.config.e2e.mjs './tests/e2e/**' ./tests/e2e/**",
    "typecheck": "npm run lint",
    "type-check": "npm run typecheck"
  },
